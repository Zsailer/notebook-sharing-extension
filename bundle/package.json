{
    "name": "jupyter_sharing",
    "version": "0.1.0",
    "description": "A bundle for the JupyterLab Sharing plugin.",
    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "homepage": "",
    "bugs": {
        "url": ""
    },
    "license": "BSD-3-Clause",
    "author": {
        "name": "Zach Sailer",
        "email": "zsailer@apple.com"
    },
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
        "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
        "style/icons/**/*.{svg}",
        "schema/*.json"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "style": "style/index.css",
    "repository": {
        "type": "git",
        "url": ""
    },
    "scripts": {
        "build": "jlpm run build:lib && jlpm run build:labextension:dev",
        "build:labextension": "jupyter labextension build .",
        "build:labextension:dev": "jupyter labextension build --development True .",
        "build:lib": "tsc",
        "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
        "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo && rimraf ../jupyter_sharing/labextension && rimraf node_modules",
        "clean:level2": "rimraf node_modules",
        "docs": "typedoc src",
        "watch": "run-p watch:src watch:labextension",
        "watch:labextension": "jupyter labextension watch .",
        "watch:src": "tsc -w"
    },
    "dependencies": {
        "@jupyter_sharing/core": "*",
        "@jupyter_sharing/events-listener-extension": "*",
        "@jupyter_sharing/share-extension": "*",
        "@jupyter_sharing/shared-extension": "*",
        "@jupyter_sharing/ui-components": "*",
        "@jupyterlab/application": "^4.1.0",
        "react": "^18.2.0"
    },
    "devDependencies": {
        "@jupyterlab/builder": "^4.1.0",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.26",
        "rimraf": "~5.0.5",
        "typescript": "~5.1.6"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "sideEffects": [
        "style/*.css",
        "style/index.js"
    ],
    "styleModule": "style/index.js",
    "jupyterlab": {
        "discovery": {
            "server": {
                "managers": [
                    "pip"
                ],
                "base": {
                    "name": "jupyter_sharing"
                }
            }
        },
        "extension": true,
        "outputDir": "../jupyter_sharing/labextension",
        "schemaDir": "schema",
        "disabledExtensions": []
    }
}
